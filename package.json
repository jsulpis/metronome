{
  "name": "metronome",
  "private": true,
  "description": "Online metronome application with a modern UI",
  "homepage": "https://github.com/jsulpis/metronome",
  "bugs": "https://github.com/jsulpis/metronome/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsulpis/metronome.git"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Julien Sulpis",
    "url": "https://twitter.com/jsulpis"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite --open",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "serve": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit -p tsconfig.global.json --composite false"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^8.2.5",
    "@vueuse/gesture": "^1.0.0",
    "howler": "^2.2.3",
    "vue": "^3.2.31",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.5.1",
    "@types/howler": "^2.2.7",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.0",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-pwa": "^0.11.13",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
